<h1>This is an index page</h1>
<p>
<%= link_to "Create new todo", new_todo_path %>
<table>
    <tr>
        <th>Name</th>
        <th>Description</th>
    </tr>
    <% @todos.each do |todo| %>
    <tr>
        <td>
            <%= todo.name %>
        </td>
        <td>
            <%= todo.description %>
        </td>
        <td>
            <%= link_to "Edit", edit_todo_path(todo.id) %> |
            <%= link_to "Show", todo_path(todo.id) %>
        </td>
    </tr>
    <% end %>
</table>